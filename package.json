{
  "name": "argumentengine",
  "version": "1.0.0",
  "description": "A project",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "postbuild-ts": "npm run lint && npm test && npm run json-schema",
    "lint": "tslint -t verbose -c  external/excelmicro_typescript_linting/tslint.json 'src/**/*.ts'",
    "json-schema": "typescript-json-schema 'src/business_model_typeorm/entity/*.ts' '*' --out dist/json-schema",
    "start": "run-p serve test",
    "postinstall": "npm run build-ts",
    "serve": "node dist/server.js",
    "test": "mocha --recursive --diff dist/test",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanplett/ArgumentEngine.git"
  },
  "author": "Evan Plett",
  "license": "ISC",
  "engines": {
    "node": "10.16.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bugs": {
    "url": "https://github.com/evanplett/ArgumentEngine/issues"
  },
  "homepage": "https://github.com/evanplett/ArgumentEngine#readme",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/jest": "^24.0.24",
    "@types/node": "^8.10.59",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "jest": "^24.8.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "3.3.3333",
    "typescript-json-schema": "^0.41.0"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "npm-run-all": "^4.1.5",
    "pg": "^7.15.1",
    "postgresql": "0.0.1",
    "reflect-metadata": "^0.1.10",
    "save-dev": "0.0.1-security",
    "typeorm": "0.2.18",
    "typescript-logging": "^0.6.4",
    "wait-on": "^3.3.0"
  }
}
