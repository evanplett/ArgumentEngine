{
  "name": "argumentengine",
  "version": "1.0.0",
  "description": "A project",
  "main": "src/dist/server.js",
  "scripts": {
    "build-ts": "tsc --showConfig --diagnostics",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "npm run serve",
    "postinstall": "npm run build-ts",
    "serve": "node src/dist/server.js",
    "test": "jest src",
    "watch-node": "nodemon src/dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanplett/ArgumentEngine.git"
  },
  "author": "Evan Plett",
  "license": "ISC",
  "engines": {
    "node": "10.16.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bugs": {
    "url": "https://github.com/evanplett/ArgumentEngine/issues"
  },
  "homepage": "https://github.com/evanplett/ArgumentEngine#readme",
  "devDependencies": {
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^23.1.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.9",
    "express": "^4.17.1",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18"
  }
}
