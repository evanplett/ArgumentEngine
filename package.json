{
   "name": "argumentengine",
   "version": "1.0.0",
   "description": "A project",
   "main": "dist/server.js",
   "scripts": {
      "build-ts": "tsc",
      "postbuild-ts": "npm test",
      "lint": "tslint -c tslint.json 'src/**/*.ts'",
      "start": "run-p serve test",
      "postinstall": "npm run build-ts",
      "serve": "node dist/server.js",
      "test": "mocha --recursive dist",
      "watch-node": "nodemon dist/server.js",
      "watch-ts": "tsc -w"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/evanplett/ArgumentEngine.git"
   },
   "author": "Evan Plett",
   "license": "ISC",
   "engines": {
      "node": "10.16.0"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   },
   "bugs": {
      "url": "https://github.com/evanplett/ArgumentEngine/issues"
   },
   "homepage": "https://github.com/evanplett/ArgumentEngine#readme",
   "devDependencies": {
      "@types/jest": "^24.0.16",
      "@types/node": "^8.0.29",
      "jest": "^24.8.0",
      "mocha": "^6.2.2",
      "nodemon": "^1.19.1",
      "prettier": "^1.18.2",
      "supertest": "^4.0.2",
      "ts-jest": "^23.1.4",
      "ts-node": "^3.3.0",
      "tslint": "^5.18.0",
      "tslint-config-prettier": "^1.18.0",
      "tslint-plugin-prettier": "^2.0.1",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/express": "^4.17.0",
      "body-parser": "^1.18.1",
      "express": "^4.15.4",
      "npm-run-all": "^4.1.5",
      "pg": "^7.3.0",
      "postgresql": "0.0.1",
      "reflect-metadata": "^0.1.10",
      "save-dev": "0.0.1-security",
      "typeorm": "0.2.18",
      "wait-on": "^3.3.0"
   }
}
